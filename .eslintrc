{
  "extends": "liferay",
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "liferay/no-dynamic-require": "off",
    "no-console": "off",
    "no-empty": "off",
    "no-for-of-loops/no-for-of-loops": "off",
    "no-return-assign": ["error", "except-parens"],
    "sort-keys": "off"
  }
}
